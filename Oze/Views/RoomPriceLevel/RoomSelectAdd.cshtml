@model List<oze.data.Entity.Vw_RoomActive>
    <div class="row Rome_steam">

        @for (int i = 0; i < Model.Count; i++)
        {
            
                <div class=" col-lg-1 col-xs-1">
                    <!-- small box -->
                    <div class="small-box bg-gray">
                        <div class="inner" style="text-align: center;">
                            <h5> @Model[i].Name </h5>
                            <input class="" @(Model[i].RoomLevel_ID.GetValueOrDefault(0) != 0?"checked":"") type="checkbox" name="@Model[i].Name" id="@Model[i].ID" />
                        </div>
                    </div>
                </div>

            

        }


    </div>

